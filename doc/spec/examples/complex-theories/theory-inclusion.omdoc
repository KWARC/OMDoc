<theory-inclusion xml:id="ti1" from="group" to="#group">
  <morphism>
    <requation>
      <legacy format="TeX">$X^{-1}$</legacy>
      <legacy format="TeX">$X$</legacy>
    </requation>
    <requation>
      <legacy format="TeX">$X$</legacy>
      <legacy format="TeX">$X^{-1}$</legacy>
    </requation>
  </morphism>
  <obligation assertion="inv.closed" induced-by="elalg.omdoc#closed.ax"/>
  <obligation assertion="inv.assoc" induced-by="elalg.omdoc#assoc.ax"/>
  <obligation assertion="inv.unit" induced-by="elalg.omdoc#unit.ax"/>
  <obligation assertion="inv.inv" induced-by="elalg.omdoc#inv.ax"/>
</theory-inclusion>  

<assertion xml:id="inv.closed">
  <FMP><legacy format="TeX">$\forall{x,y}.x^{-1}\in{M}\wedge
      y^{-1}\in{M}\Rightarrow x^{-1}\circ y^{-1}\in{M}$</legacy>
    </FMP>
</assertion>
<assertion xml:id="inv.assoc">
  <FMP><legacy format="TeX">$\forall{x,y,z}.x^{-1}\in{M}\wedge y^{-1}\in{M}\wedge
      z^{-1}\in{M}\Rightarrow (x^{-1}\circ y^{-1}) \circ
      z^{-1}=x^{-1}\circ(y^{-1}\circ z^{-1})$</legacy>
  </FMP>
</assertion>
<assertion xml:id="inv.unit">
  <FMP><legacy format="TeX">$\forall{x}.x^{-1}\in{M}\Rightarrow x^{-1}\circ e=
      x^{-1}$</legacy></FMP>
    </assertion>
<assertion xml:id="inv.inv">
  <FMP><legacy format="TeX">$\forall{x,y}.x^{-1}\in{M}\wedge
      y^{-1}\in{M}\Rightarrow x^{-1}\circ x=e$</legacy></FMP>
</assertion>

