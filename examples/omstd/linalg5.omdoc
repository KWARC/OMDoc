      <OMS cd="linalg2" name="vector"/>
      <OMA>
	<OMS cd="linalg2" name="vector"/>
	<OMI>4</OMI>
	<OMI>8</OMI>
	<OMI>12</OMI>
	<OMI>15</OMI>
      </OMA>
      <OMA>
	<OMS cd="linalg2" name="vector"/>
	<OMI>1</OMI>
	<OMI>5</OMI>
	<OMI>9</OMI>
	<OMI>13</OMI>
	<OMI>16</OMI>
      </OMA>
      <OMA>
	<OMS cd="linalg2" name="vector"/>
	<OMI>2</OMI>
	<OMI>6</OMI>
	<OMI>10</OMI>
	<OMI>14</OMI>
      </OMA>
      <OMA>
	<OMS cd="linalg2" name="vector"/>
	<OMI>3</OMI>
	<OMI>7</OMI>
	<OMI>11</OMI>
      </OMA>
    </OMA>
  </OMA>
</OMOBJ>
</example>
<assertion xml:id="symmetric-prop-1" type="lemma">
  <CMP>
    the sum of a symmetric matrix and its transpose is symmetric
  </CMP>
  <FMP>
    <OMOBJ xmlns="http://www.openmath.org/OpenMath">
      <OMA>
	<OMS cd="relation1" name="eq"/>
	<OMA>
	  <OMS cd="arith1" name="plus"/>
	  <OMA>
	    <OMS cd="linalg5" name="symmetric"/>
	    <OMV name="VV1"/>
	  </OMA>
	  <OMA>
	    <OMS cd="linalg1" name="transpose"/>
	    <OMA>
	      <OMS cd="linalg5" name="symmetric"/>
	      <OMV name="VV1"/>
	    </OMA>
	  </OMA>
	</OMA>
	<OMA>
	  <OMS cd="linalg5" name="symmetric"/>
	  <OMV name="VV2"/>
	</OMA>
      </OMA>
    </OMOBJ>
  </FMP>
</assertion>
<assertion xml:id="symmetric-prop-3" type="lemma">
  <CMP> for a symmetric matrix M, M = transpose M </CMP>
  <FMP>
    <OMOBJ xmlns="http://www.openmath.org/OpenMath">
      <OMA>
	<OMS cd="relation1" name="eq"/>
	<OMA>
	  <OMS cd="linalg5" name="symmetric"/>
	  <OMV name="VV"/>
	</OMA>
	<OMA>
	  <OMS cd="linalg1" name="transpose"/>
	  <OMA>
	    <OMS cd="linalg5" name="symmetric"/>
	    <OMV name="VV"/>
	  </OMA>
	</OMA>
      </OMA>
    </OMOBJ>
  </FMP>
</assertion>
<assertion xml:id="symmetric-prop-5" type="lemma">
  <CMP>
    the dimension of a symmetric matrix = the length of the vector
    which defines it
  </CMP>
  <FMP>
    <OMOBJ xmlns="http://www.openmath.org/OpenMath">
      <OMA>
	<OMS cd="relation1" name="eq"/>
	<OMA>
	  <OMS cd="linalg4" name="rowcount"/>
	  <OMA>
	    <OMS cd="linalg5" name="symmetric"/>
	    <OMV name="VV"/>
	  </OMA>
	</OMA>
	<OMA>
	  <OMS cd="linalg4" name="size"/>
	  <OMV name="VV"/>
	</OMA>
      </OMA>
    </OMOBJ>
  </FMP>
</assertion>
<example xml:id="symmetric-ex-7" type="for" for="#symmetric">
  <CMP>
    An example to represent the symmetric matrix:
    [[1,2,3,4]
    [2,5,6,7]
    [3,6,8,9]
    [4,7,9,10]]</CMP>

<OMOBJ xmlns="http://www.openmath.org/OpenMath">
  <OMA>
    <OMS cd="linalg5" name="symmetric"/>
    <OMA>
      <OMS cd="linalg2" name="vector"/>
      <OMA>
	<OMS cd="linalg2" name="vector"/>
	<OMI> 1 </OMI>
	<OMI> 2 </OMI>
	<OMI> 3 </OMI>
	<OMI> 4 </OMI>
      </OMA>
      <OMA>
	<OMS cd="linalg2" name="vector"/>
	<OMI> 5 </OMI>
	<OMI> 6 </OMI>
	<OMI> 7 </OMI>
      </OMA>
      <OMA>
	<OMS cd="linalg2" name="vector"/>
	<OMI> 8 </OMI>
	<OMI> 9 </OMI>
      </OMA>
      <OMA>
