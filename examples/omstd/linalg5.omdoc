		<OMS cd="linalg4" name="rowcount"/>
		<OMV name="M"/>
	      </OMA>
	      <OMA>
		<OMS cd="linalg4" name="columncount"/>
		<OMV name="M"/>
	      </OMA>
	    </OMA>
	  </OMA>
	  <OMA id="zero-prop-3.2">
	    <OMS cd="relation1" name="eq"/>
	    <OMA>
