	    <OMI> 4 </OMI>
	  </OMA>
	  <OMA>
	    <OMS name="times" cd="arith1"/>
	    <OMI> -4 </OMI>
	    <OMA>
	      <OMS name="power" cd="arith1"/>
	      <OMV name="x"/>
	      <OMI> 2 </OMI>
	    </OMA>
	  </OMA>
	</OMA>
      </OMA>
      <OMA>
	<OMS name="factored" cd="poly"/>
	<OMA>
	  <OMS name="power" cd="poly"/>
	  <OMV name="x"/>
	  <OMI> 2 </OMI>
	</OMA>
	<OMA>
	  <OMS name="power" cd="poly"/>
	  <OMA>
	    <OMS name="plus" cd="arith1"/>
	    <OMV name="x"/>
	    <OMI> 2 </OMI>
	  </OMA>
	  <OMI> 1 </OMI>
	</OMA>
	<OMA>
	  <OMS name="power" cd="poly"/>
