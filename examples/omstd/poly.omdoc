</dc:description>

<type>
    <OMOBJ xmlns="http://www.openmath.org/OpenMath">
      <OMA>
	<OMS name="mapsto" cd="sts"/>
	<OMS name="polynomial_ring" cd="polysts"/>
	<OMV name="FactoredPolynomial"/>
      </OMA>
    </OMOBJ>
  </type>
</symbol>
