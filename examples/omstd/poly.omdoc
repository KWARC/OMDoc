	    <OMS name="minus" cd="arith1"/>
	    <OMV name="x"/>
	    <OMI> 2 </OMI>
	  </OMA>
	  <OMI> 1 </OMI>
	</OMA>
      </OMA>
    </OMA>
  </OMOBJ>
</example>
